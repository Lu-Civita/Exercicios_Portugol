programa{
	
	funcao inicio(){
		caracter letra
		escreva ("Digite uma letra: ")
		leia (letra)
		escolha (letra){
			caso 'a':
			escreva ("VOGAL")
			pare
				caso 'e':
			escreva ("VOGAL")
			pare
				caso 'i':
			escreva ("VOGAL")
			pare
				caso 'o':
			escreva ("VOGAL")
			pare
				caso 'u':
			escreva ("VOGAL")
			caso contrario:
			escreva ("É CONSOANTE")
		}
	}
}
